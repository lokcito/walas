// Declarative //
pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                echo 'Test 1 2 x Building..'
            }
        }
        stage('Test') {
            steps {
                echo 'Test a 3 y Testing..'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Test b 4 z Deploying....'
            }
        }
    }
}