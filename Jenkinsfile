// Declarative //
pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                echo 'Test 1 Building..'
            }
        }
        stage('Test') {
            steps {
                echo 'Test a Testing..'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Test b Deploying....'
            }
        }
    }
}